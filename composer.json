{
    "name": "bpez/infuse",
    "type": "library",
    "description": "Infuse is a laravel admin package for generating administration style interfaces",
    "version": "3.0.0",
    "keywords": [
        "laravel 4", 
        "laravel", 
        "admin", 
        "scaffold"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Perez",
            "email": "perezjbryan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*",
        "mjohnson/transit": "1.5.*",
        "toddish/verify": "2.3.*",
        "clouddueling/mysqldump-php": "1.0.*",
        "intervention/image": "1.5.*",
        "nesbot/Carbon": "1.8.*",
        "barryvdh/laravel-debugbar": "1.*",
        "codesleeve/asset-pipeline": "2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models"
        ],
        "psr-0": {
            "Bpez\\Infuse\\": "src/"
        }
    },
    "minimum-stability": "dev"
}