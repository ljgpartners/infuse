    "keywords":["laravel 4", "laravel", "admin", "scaffold"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Perez",
            "email": "perezjbryan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "mjohnson/transit": "1.3.*",
        "dg/mysql-dump": "dev-master"
    },
    
    "autoload": {
