{
    "name": "bpez/infuse",
    "description": "Infuse is a laravel admin package for generating administration style interfaces",
    "keywords": [
        "laravel 4", 
        "laravel", 
        "admin", 
        "scaffold"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Perez",
            "email": "perezjbryan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "bpez/transit": "infuse-patch",
        "toddish/verify": "2.*",
        "clouddueling/mysqldump-php": "1.0.*",
        "intervention/image": "1.5.*",
        "nesbot/Carbon": "1.8.*",
        "barryvdh/laravel-debugbar": "1.*",
        "way/generators": "2.*",
        "bpez/munee": "1.5.*",
        "paquettg/php-html-parser": "1.6.3"
    },
    "repositories": [
    {
        "type": "vcs",
        "url": "https://bitbucket.org/perezjbryan/transit.git"
    }
    ],
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models",
            "src/PHPCrawler"
        ],
        "psr-0": {
            "Bpez\\Infuse\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
