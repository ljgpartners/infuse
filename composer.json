{
    "name": "bpez/infuse",
    "type": "library",
    "description": "Infuse is a laravel admin package for generating administration style interfaces",
    "version": "2.0.0",
    "keywords": [
        "laravel 4", 
        "laravel", 
        "admin", 
        "scaffold"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Perez",
            "email": "perezjbryan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "illuminate/support": "4.1.*",
        "mjohnson/transit": "1.3.*",
        "zizaco/entrust": "dev-master"
    },
    
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models"
        ],
        "psr-0": {
            "Bpez\\Infuse\\": "src/"
        }
    },
    "minimum-stability": "stable"
}