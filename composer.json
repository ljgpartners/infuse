{
    "name": "bpez/infuse",
    "type": "library",
    "description": "Infuse is a laravel admin package for generating administration style interfaces",
    "keywords": [
        "laravel 4",
        "laravel",
        "admin",
        "scaffold"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*",
        "mjohnson/transit": "1.5.*",
        "toddish/verify": "2.*",
        "clouddueling/mysqldump-php": "1.0.*",
        "intervention/image": "1.5.*",
        "nesbot/Carbon": "1.8.*",
        "barryvdh/laravel-debugbar": "1.*",
        "way/generators": "2.*",
        "meenie/munee": "1.5.20",
        "paquettg/php-html-parser": "1.6.3"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models",
            "src/PHPCrawler"
        ],
        "psr-0": {
            "Bpez\\Infuse\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
