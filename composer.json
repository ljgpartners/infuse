{
    "name": "bpez/infuse",
    "description": "Infuse is a laravel admin package for generating administration style interfaces",
    "keywords": [
        "laravel 5",
        "laravel",
        "admin",
        "scaffold"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "bpez/transit": "1.6.*",
        "toddish/verify": "5.0.0",
        "nesbot/Carbon": "1.*",
        "barryvdh/laravel-debugbar": "2.3.*",
        "intervention/image": "2.1.2",
        "league/flysystem-aws-s3-v3": "~1.0",
        "paquettg/php-html-parser": "1.6.3",
        "99designs/phumbor": "1.1.0"
    },
    "require-dev": {
      "fzaninotto/faker": "~1.4",
      "mockery/mockery": "0.9.*",
      "phpunit/phpunit": "~4.0",
      "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "migrations",
            "seeds",
            "controllers",
            "models",
            "PHPCrawler",
            "features/bootstrap"
        ],
        "psr-4": {
            "Bpez\\Infuse\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
