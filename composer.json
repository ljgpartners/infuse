{
    "name": "bpez/infuse",
    "description": "Infuse is a laravel admin package for generating administration style interfaces",
    "keywords": [
        "laravel 5",
        "laravel",
        "admin",
        "scaffold"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Perez",
            "email": "perezjbryan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "bpez/transit": "1.6.*",
        "toddish/verify": "dev-laravel5",
        "nesbot/Carbon": "1.*",
        "barryvdh/laravel-debugbar": "2.*",
        "intervention/image": "2.1.2",
        "league/flysystem-aws-s3-v2": "~1.0",
        "paquettg/php-html-parser": "1.6.3",
        "99designs/phumbor": "1.1.0"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0",
      "phpspec/phpspec": "~2.1",
      "behat/behat": "~3.0@dev",
      "behat/mink": "~1.7@dev",
      "behat/mink-extension": "~2.0@dev",
      "laracasts/behat-laravel-extension": "dev-master"
    },
    "autoload": {
        "classmap": [
            "migrations",
            "seeds",
            "controllers",
            "models",
            "PHPCrawler",
            "features/bootstrap"
        ],
        "psr-4": {
            "Bpez\\Infuse\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
